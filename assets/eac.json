{
  "Applications": {
    "$1": {
      "Application": {
        "Description": "A Svelte starter app deployed to Fathym via an NPM package.",
        "Name": "Svelte Starter - NPM Launch"
      },
      "LookupConfig": {
        "PathRegex": "/.*"
      },
      "LowCodeUnit": {
        "Package": "@fathym-it/public-web-svelte",
        "Version": "latest",
        "Type": "NPM"
      },
      "Processor": {
        "BaseHref": "/",
        "DefaultFile": "index.html",
        "Type": "DFS"
      }
    }
  }
}